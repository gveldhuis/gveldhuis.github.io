lick */\n    const handleMobileClick = () => {\n        setClick(!click);\n    };\n    \n\n    /*Toggles Side-Nav Menu Based on Screen Size*/\n    const resizeMenu = () => {\n        if(window.innerWidth >= 1060) {\n            setClick(false);      \n        } \n    };\n\n    /*Toggles color of NavMenu based on scroll location on page*/\n    const toggleNavMenu = () => {\n        if(window.pageYOffset < 50) {\n            setTop(true);\n        } else {\n            setTop(false);\n        }\n    }\n\n    \n\n    \n    let currLocation = useLocation().pathname;\n    const pages = ['aboutme', 'experience', 'work', 'contact']\n\n    useEffect(() => {\n\n        if(currLocation === '/') {\n            document.getElementById('aboutme').style.fontWeight = 800;\n            document.getElementById('experience').style.fontWeight = 300;\n            document.getElementById('work').style.fontWeight = 300;\n            document.getElementById('contact').style.fontWeight = 300;\n\n        }\n        else {\n            for (const route of pages) {\n                let test = props.page.includes(route);\n                if(test) {\n                    document.getElementById(route).style.fontWeight = 800;\n                }\n                else {\n                    document.getElementById(route).style.fontWeight = 300;\n                }\n            }\n        }\n    });\n\n    window.addEventListener('resize', resizeMenu);\n    // let testVar = document.getElementById('unique2');\n    // console.log(testVar);\n    // testVar.addEventListener('scroll', toggleNavMenu);\n\n    // document.addEventListener('wheel', toggleNavMenu);\n    // window.addEventListener('scroll', toggleNavMenu);\n\n    \n\n\n    \n    return (\n        <nav className={top ? 'NavBar' : 'NavBar offset'}> \n            <h1 className='NavBar-nameLogo'>GRANT VELDHUIS</h1>\n            <div className='NavBar-menuicon' onClick={handleMobileClick}>\n                <i className=\"fas fa-bars\"></i>\n            </div>\n                <div className={click ? 'NavBar-list active' : 'NavBar-list'}>\n                    <Link onClick={closeMobileMenu} to='/' id='aboutme'>About</Link>\n                    <Link onClick={closeMobileMenu} to='/experience' id='experience'>Experience</Link>\n                    <Link onClick={closeMobileMenu} to='/work' id='work'>My Work</Link>\n                    <Link onClick={closeMobileMenu} to='/contact' id='contact'>Contact</Link>\n                </div>\n        </nav>\n        );\n\n        \n\n}","/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Intro/Intro.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/AboutMe/AboutMe.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Experience/Experience.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Skills/Skills.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Work/Work.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Contact/Contact.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/routes.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Work/WorkData.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/WorkContent/WorkContent.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/ExperienceContent/ExperienceContent.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/Experience/ExperienceData.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/WorkCard/WorkCard.js",[],"/Users/grantveldhuis/Desktop/UMichigan/personalwebsite/src/components/ExperienceCard/ExperienceCard.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":28,"column":11,"nodeType":"81","messageId":"82","endLine":28,"endColumn":24},"no-native-reassign",